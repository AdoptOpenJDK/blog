{
  "env": {
      "es6": true,
      "node": true,
      "browser": true
  },
  "extends": "eslint:recommended",
  "plugins": [
      "react"
  ],
  "rules": {
      "no-console": 0,
      "indent": [
          2,
          2
      ],
      "quotes": [
          2,
          "double"
      ],
      "semi": [
          2,
          "always"
      ],
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/react-in-jsx-scope": 1
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  }
}